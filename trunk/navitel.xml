<?xml version="1.0"?>
<translations>

  <translation>
    <name>Православный храм</name>
    <match mode="and" type="node">
      <tag k="amenity" v="place_of_worship"/>
      <tag k="religion" v="christian"/>
      <tag k="denomination" v="orthodox"/>
    </match>
    <output>
      <copy-unmatched/>
      <tag k="mp_code" v="0xF101"/>
    </output>
   </translation>

