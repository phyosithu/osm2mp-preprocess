<?xml version="1.0"?>
<translations>
  <translation>
    <name>Ресторан (азиатская кухня)</name>
    <match mode="and">
      <tag k="amenity" v="restaurant"/>
      <tag k="cuisine" v="asian"/>
    </match>
    <output>
      <copy-unmatched/>
      <tag k="mp_code" v="0x2a02"/>
    </output>
   </translation>

